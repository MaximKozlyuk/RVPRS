<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE appconfig PUBLIC "" "appconfig.dtd">
<appconfig>
    <rmi>
        <server>
            <registry>
                <createregistry>yes</createregistry>
                <registryaddress>127.0.0.1</registryaddress>
                <registryport>1099</registryport>
            </registry>
        </server>
        <client>
            <policypath>client.policy</policypath>
            <usecodebaseonly>yes</usecodebaseonly>
        </client>
        <classprovider>http://www.yourhost.free.ru/cp/cp.jar</classprovider>
    </rmi>
    <datasource>
        <classname>CLASSNAME</classname>
        <drivertype>org.postgresql.Driver</drivertype>
        <hostName>127.0.0.1</hostName>
        <port>5432</port>
        <DBName>library</DBName>
        <user>max</user>
        <pass></pass>
    </datasource>
</appconfig>
